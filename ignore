    "\n",
    "# Use a correct file path (replace with your actual path)\n",
    "speech_path = r'C:\\Users\\Dana Councilman\\Downloads\\speeches.txt'  \n",
    "\n",
    "def find_top_countries(speech_path, countries_list):\n",
    "    '''Find the top five countries mentioned in a speech.'''\n",
    "    \n",
    "    # Compile a regex pattern for country names (case insensitive)\n",
    "    pattern = r'\\b(?:' + '|'.join(re.escape(country) for country in countries_list) + r')\\b'\n",
    "    \n",
    "    # Counter to hold country mentions\n",
    "    country_counter = Counter()\n",
    "\n",
    "    try:\n",
    "        # Open the speech text file directly\n",
    "        with open(speech_path, 'r', encoding='utf-8', errors='ignore') as file:\n",
    "            content = file.read()\n",
    "            \n",
    "            # Find all matches of the country names in the content\n",
    "            matches = re.findall(pattern, content, flags=re.IGNORECASE)\n",
    "            country_counter.update([match for match in matches])\n",
    "\n",
    "    except Exception as e:\n",
    "        print(f\"Could not read {speech_path}: {e}\")\n",
    "\n",
    "    return country_counter\n",
    "\n",
    "# Find the top countries mentioned\n",
    "country_counts = find_top_countries(speech_path, countries_list)\n",
    "\n",
    "# Print the top five countries\n",
    "top_countries = country_counts.most_common(5)\n",
    "if top_countries:\n",
    "    print(\"Top 5 countries mentioned in the speech:\")\n",
    "    for country, count in top_countries:\n",
    "        print(f\"{country}: {count} mentions\")\n",
    "else:\n",
    "    print(\"No countries mentioned in the speech.\")"
   ]
  },
